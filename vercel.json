{
  "version": 2,
  "name": "ordo-ab-chao",
  "builds": [
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/login.html",
      "dest": "/web/login.html"
    },
    {
      "src": "/index.html",
      "dest": "/web/index.html"
    },
    {
      "src": "/projects.html",
      "dest": "/web/projects.html"
    },
    {
      "src": "/manifest.json",
      "dest": "/web/manifest.json"
    },
    {
      "src": "/service-worker.js",
      "dest": "/web/service-worker.js"
    },
    {
      "src": "/config.js",
      "dest": "/web/config.js"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico))",
      "dest": "/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
